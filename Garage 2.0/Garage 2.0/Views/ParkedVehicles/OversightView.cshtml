@model IEnumerable<Garage_2._0.Models.ParkedVehicle>

@{
    ViewBag.Title = "Welcome to an oversightview of the garage.";
}

<h2>Oversightview</h2>

Right now there is @Model.Count() vehicles in the garage.

 
<table class="table">
    <tr>
        <th>      @Html.DisplayNameFor(model => model.Type)  </th>
       
        <th>      @Html.DisplayNameFor(model => model.RegNo) </th>
       
        <th>      @Html.DisplayNameFor(model => model.Color) </th>
       
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>   @Html.DisplayFor(modelItem => item.Type)   </td>
        
        <td>   @Html.DisplayFor(modelItem => item.RegNo)  </td>
        
        <td>   @Html.DisplayFor(modelItem => item.Color)  </td>
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
